100%;\r\n    background-color: $bcolor-black;\r\n    .header__wrapper{\r\n        display: flex;\r\n        flex-wrap: wrap;\r\n        align-items: center;\r\n        justify-content: space-between;\r\n}\r\n}\r\nnav{\r\n    max-width: 570px;\r\n    max-height: 75px;\r\n    width: 100%;\r\n\r\n    ul{\r\n        display: flex;\r\n        flex-wrap: wrap;\r\n        height: 75px;\r\n    }\r\n\r\n    li{\r\n        width: 20%;\r\n        height: 100%;\r\n        display: flex;\r\n        justify-content: center;\r\n        align-items: center;\r\n    }\r\n\r\n    li:last-child{\r\n        margin-right: 0;}\r\n    a{\r\n        font-family: 'Open Sans';\r\n        font-style: normal;\r\n        font-weight: 600;\r\n        font-size: 14px;\r\n        line-height: 19px;\r\n        text-transform: capitalize;\r\n        text-decoration: none;\r\n\r\n        color: $tcolor-white;\r\n    }\r\n    li:hover{\r\n        background: $bcolor-light_orange;\r\n    }\r\n    li:active{\r\n        background: $bcolor-orange_button_active;\r\n    }\r\n    a:hover,:active{\r\n        text-transform: uppercase;\r\n    }\r\n}\r\n    ","scss\\variable.scss":"// text color \r\n$tcolor-white: #FFFFFF;\r\n$tcolor-black_grey: #333333;\r\n$tcolor-light_grey: #86878B;\r\n$tcolor-black: #000000;\r\n// backgraund color\r\n$bcolor-black: #000000;\r\n$bcolor-black_grey: #333333;\r\n$bcolor-light_grey:#E2E2E2;\r\n$bcolor-white_grey: #F5F5F5;\r\n$bcolor-white: #FFFFFF;\r\n$bcolor-light_orange: #EC7979;\r\n$bcolor-orange_button_active: #AB4949;\r\n","scss\\main.scss":".intro{\r\n    min-height: 100vh;\r\n    background-image: url(../img/image__intro.png);\r\n    background-repeat: no-repeat;\r\n    background-position: 50% 50%;\r\n    background-size: cover; \r\n}\r\n.intro__wrapper{\r\n    display: flex;\r\n    flex-wrap: wrap;\r\n    justify-content: space-between;\r\n    align-items: center;\r\n    padding-top: 12%;\r\n}\r\n.intro__information--card{\r\n    display: flex;\r\n    flex-direction: column;\r\n    .intro__logo{\r\n        width: 270px;\r\n        height: 100px;\r\n        margin-bottom: 50px;\r\n    }\r\n    h1{\r\n        width: 670px;\r\n        height: 168px;\r\n        margin-bottom: 55px;\r\n\r\n        font-family: 'Raleway';\r\n        font-style: normal;\r\n        font-weight: 500;\r\n        font-size: 48px;\r\n        line-height: 56px;\r\n        \r\n        color: $tcolor-white;\r\n    }\r\n    .separator{\r\n        width: 170px;\r\n        margin-bottom: 42px;\r\n        border: 1px solid $tcolor-white;\r\n\r\n    }\r\n    .intro__subtittle{\r\n        width: 270px;\r\n        height: 42px;\r\n        margin-bottom: 42px;\r\n        color: $tcolor-white;\r\n\r\n    }\r\n    .btn__download{\r\n        width: 270px;\r\n        height: 58px;\r\n    }\r\n}\r\n\r\n    "},"lineCount":null}},"error":null,"hash":"bc5ce745a6854bc0a232f051adac6562","cacheData":{"env":{}}}